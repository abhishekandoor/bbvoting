SELECT `U`.`id`, `U`.`username`, `U`.`email`, `U`.`is_sampoorna`, `MG`.`mngmnt_code`, `US`.`descp`, `U`.`user_status`, (CASE WHEN U.activated ="1" THEN "Active" WHEN U.banned = "1" THEN "De-Active" END) as current_status, CONCAT_WS(" ", `UF`.`name`, `UF`.`middle_name`, UF.last_name)AS name, `MD`.`designation_name` as `designation`, `UG`.`usergroup`, `MDS`.`district_name`, `ME`.`edu_district_name`, `MS`.`sub_district_name`, `U`.`office_id` FROM `users` `U` RIGHT JOIN`user_profiles` `UF` ON `UF`.`user_id` = `U`.`id` RIGHT JOIN`AASF_User_Status` `US` ON `US`.`id`=`U`.`user_status` RIGHT JOIN`master_designation` `MD` ON `MD`.`desig_id` = `U`.`designation_id` RIGHT JOIN`rbac_group` `UG` ON `UG`.`id` = `U`.`user_group_id`  RIGHT JOIN`master_district` `MDS` ON `MDS`.`district_code` = `U`.`district_id`  RIGHT JOIN`master_edudistricts` `ME` ON `ME`.`id` = `U`.`edudistrict_id`  RIGHT JOIN`master_subdistricts` `MS` ON `MS`.`id` = `U`.`subdistrict_id`  RIGHT JOIN`AASF_Management` `MG` ON `MG`.`id` = `U`.`school_management_id` WHERE `subdistrict_id` = '403' AND `U`.`id` != '140' AND `U`.`office_id` IN('1007', '1012') AND `U`.`is_transfered` =0 ORDER BY `activated` DESC